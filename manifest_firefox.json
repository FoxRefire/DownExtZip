{
    "name": "DownExtZip",
    "description": "Download browser extension's source code as zip",
    "version": "1.2",
    "manifest_version": 3,
    "icons": {
        "128": "icon.png"
    },
    "action": {
        "default_title": "DownExtZip",
        "default_popup": "popup.html"
    },
    "permissions":["tabs"],
    "browser_specific_settings": {
        "gecko": {
            "id": "DownExtZip@DownExtZip"
        }
    }
}
